package HeapProblems;

public class HeapInsertion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] input = {12,32,1,122,13};
		
	}
	
	class HeapCreation{
		int insertdata;
		int [] HeapCheck;
		
		public HeapCreation(){
			this.insertdata = 0;
			this.HeapCheck = null;
		}
		public HeapCreation(int insertdata){
			this.insertdata = insertdata;
			this.HeapCheck = null;
		}
	}

}
