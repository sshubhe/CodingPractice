
public class fibonacci {
	
	public static void main(String args[]){
		
		int i=0;
		try{
			i = fib(1231);
		}
		catch(Exception e){
			System.out.println(e);
		}
		System.out.println(i);
		
		
	}
	
	public static int fib(int n ){
		if(n <= 1 ){
			return n;
		}
	
		return fib(n-1) + fib(n-2);
	}

}
