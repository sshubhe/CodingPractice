
public class Palndrome {

	
	public static void main(String[] args) {
		// Palendrome
		String Check1 = "Tiger";
		for(int i = 0,j = Check1.length()-1; i < Check1.length() && j>=0;i++,j--){
				
				if( Check1.charAt(i) == Check1.charAt(j))
				{
				  System.out.println( Check1.charAt(i) + " " + Check1.charAt(j));
				}
			}
		}
	}
